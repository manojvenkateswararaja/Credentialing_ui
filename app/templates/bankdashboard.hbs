
{{partial 'head'}}
{{#if showDashboard }}
<script>
   $(document).ready(function() {
       // Show or hide the sticky footer button
       $(window).scroll(function() {
           if ($(this).scrollTop() > 200) {
               $('.go-top').fadeIn(200);
           } else {
               $('.go-top').fadeOut(200);
           }
       });
       // Animate the scroll to top
       $('.go-top').click(function(event) {
           event.preventDefault();
           $('html, body').animate({
               scrollTop: 0
           }, 300);
       });
      
   });
</script>
   <div id="cont-lock" style="text-align:left ;color:white ">
      <h1><b>Bank Dashboard</b></h1>
   </div>
<div class="container-fluid" style="background-color:#808080">
   <br/>
   <p style="text-align:center;color:black"><b>Total Request</b></p>
      <p class="head-request"><b>ALL Request</b></p>
   <div class="dash-container" style="background-color:#2d8cff">
     
    
         {{#each id as |item|}}
         <ul>
             <div style="background:lightgrey">
            Requestid<a href="" {{action 'userdetails' item }}><b style="color:#2d8cff ">{{item.requestid}}</b></a>
            <div class="vl">{{ item.transactionstring.firstname}}</div> 
            <div class="vl2 ">{{ item.transactionstring.propertyType}}</div>
            <div class="vl3 ">{{ item.transactionstring.CompanyName}}</div>
            </div>
            </ul>
            {{/each}}
         
   
   </div>
</div>
<a href="#" class="go-top">Scroll to Top</a>
{{/if}}